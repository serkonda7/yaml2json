{
	"name": "@serkonda7/yaml2json",
	"version": "0.6.0",
	"description": "Utility script for converting YAML files to JSON.",
	"author": "Lukas Neubert <serkonda7@pm.me>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/serkonda7/yaml2json.git"
	},
	"engines": {
		"node": "14 || >=16"
	},
	"bin": {
		"yaml2json": "dist/yaml2json.js"
	},
	"scripts": {
		"add-shebang": "echo -e \"#!/usr/bin/env node\n$(cat dist/yaml2json.js)\" > dist/yaml2json.js",
		"compile": "tsc -p ./ && npm run add-shebang",
		"watch": "tsc -watch -p ./",
		"lint": "eslint .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"publish": "npm run compile && npm publish"
	},
	"dependencies": {
		"js-yaml": "~4.1.0"
	},
	"devDependencies": {
		"@types/js-yaml": "~4.0.5",
		"@types/node": "^14.18.26",
		"@typescript-eslint/eslint-plugin": "~5.35.1",
		"@typescript-eslint/parser": "~5.35.1",
		"eslint": "~8.22.0",
		"typescript": "~4.7.4"
	}
}
